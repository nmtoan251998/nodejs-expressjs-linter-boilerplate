{
  "name": "backend-apis-nodejs-app-boilerplate",
  "version": "0.1.0",
  "description": "Backend APIs Nodejs App boilerplate",
  "main": "src/index.js",
  "scripts": {
    "linter:watch": "node node_modules/eslint-watch/bin/esw -w --fix -c ./.eslintrc.json",
    "dev:server": "nodemon ./src/index.js",
    "start": "concurrently -p [{name}] -n \"Server,Linter\" -c \"bgCyan.bold,bgRed.bold\" \"npm run dev:server\" \"npm run linter:watch\"",
    "start-docker": "docker-compose up",
    "apidoc": "apidoc -i src/ -o docs/"
  },
  "keywords": [
    "nodejs",
    "expressjs",
    "eslint",
    "prettier",
    "docker",
    "mongodb",
    "boilerplate",
    "api",
    "apidoc"
  ],
  "author": "noat",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "date-fns": "^2.8.1",
    "dotenv": "^8.2.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "http-status": "^1.4.2",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^3.3.5",
    "morgan": "^1.9.1",
    "prettier": "^1.19.1"
  },
  "devDependencies": {
    "apidoc": "^0.19.0",
    "babel-eslint": "^10.0.3",
    "concurrently": "^5.0.1",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-watch": "^6.0.1",
    "nodemon": "^2.0.1"
  }
}
